doctype 5
html(lang = "en")
head
	title= header.pageTitle
	meta(charset="utf-8")
	meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
	meta(name="keywords", content="#{header.metaKeywords}")
	meta(name="description", content="#{header.metaDescription}")
	meta(name="viewport", content="width=device-width")

	each url in header.styleSheets
		link(rel="stylesheet", type="text/css", href="#{url}")

	each url in header.javascripts
		script(type="text/javascript", src="#{url}")
		
	script(type="text/javascript", data-main="#{header.requireApp}", src="#{header.requireUrl}")

body
	#header
		h1 #{header.pageTitle}

	